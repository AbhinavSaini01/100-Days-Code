
#include<stdio.h>
#include<conio.h>

int fact(int n)
{
    if(n==0)
    {
        return 1;
    }
    else if(n==1)
    {
        return 1;
    }
    else
    {
        return n*fact(n-1);
    }
}

void main()
{
    int n;
    printf("Enter a number");
    scanf("%d",&n);
    
    int result=fact(n);
    printf("facrorial of %d is %d",n,result);

}
